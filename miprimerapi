MyPrimerApi.py

from flask import Flask, jsonify

app = Flask(_name_)

tasks = [
  {"id":1, "nombre":"Omi", "enable":True},
  {"id":2, "nombre":"Jess", "enable":False},
  {"id":3, "nombre":"Liz", "enable":True},
  {{"id":2, "nombre":"Tom", "enable":False},
]

@app.route('/tasks', methods=['GET'])
def get taks():
    return jsonify(tasks)

if _name_ == '_main_':
    app.run(debug=True)



